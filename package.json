{
  "name": "yuna",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node src/Index.ts",
    "start:dev": "ts-node-dev src/Index.ts",
    "start:prod": "node dist/src/Index.js",
    "build": "rimraf dist && npm run lint && tsc",
    "build:start:prod": "npm run build && npm run start:prod",
    "watch": "tsc -w",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prisma:dev": "npx prisma migrate dev --name update & npx prisma generate",
    "prisma:deploy": "npx prisma migrate deploy --preview-feature",
    "prisma:generate": "npx prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DasIschBims/yuna.git"
  },
  "keywords": [
    "discord-bot",
    "discord.js",
    "discord",
    "typescript"
  ],
  "author": "DasIschBims",
  "license": "ISC",
  "dependencies": {
    "@discordjs/rest": "^1.7.1",
    "@prisma/client": "^4.13.0",
    "@types/glob": "^8.1.0",
    "@types/node": "^18.15.11",
    "canvas": "2.11.2",
    "chart.js": "3.9.1",
    "chartjs-adapter-dayjs-3": "1.2.3",
    "chartjs-node-canvas": "4.1.6",
    "colors": "^1.4.0",
    "common-tags": "^1.8.2",
    "dayjs": "1.10.8",
    "discord-api-types": "^0.37.42",
    "discord.js": "^14.11.0",
