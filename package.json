    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.5",
    "@types/gradient-string": "^1.1.2",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "prisma": "^4.13.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
